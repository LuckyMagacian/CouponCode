    <dubbo:service interface="com.lanxi.couponcode.spi.service.MerchantService" ref="merchantControllerService" registry="zk"></dubbo:service>
    <dubbo:service interface="com.lanxi.couponcode.spi.service.ShopService" ref="shopControllerService" registry="zk"></dubbo:service>
    <dubbo:service interface="com.lanxi.couponcode.spi.service.LoginService" ref="loginControllerService" registry="zk"></dubbo:service>
    <dubbo:service interface="com.lanxi.couponcode.spi.service.OrderService" ref="orderControllerService" registry="zk"></dubbo:service>
    <dubbo:service interface="com.lanxi.couponcode.spi.service.CommodityService" ref="commodityControllerService" registry="zk"></dubbo:service>
    
    
    <dubbo:reference interface="com.lanxi.couponcode.spi.service.AccountService" id="accountControllerServiceRef" check="false" registry="zk"></dubbo:reference>
