    <dubbo:reference interface="com.lanxi.couponcode.spi.service.VerificationRecordService" id="verificationRecordControllerServiceRef" check="false" registry="zk"></dubbo:reference>
    <dubbo:reference interface="com.lanxi.couponcode.spi.service.MerchantService" id="merchantControllerServiceRef" check="false" registry="zk"></dubbo:reference>
    <dubbo:reference interface="com.lanxi.couponcode.spi.service.ShopService" id="shopControllerServiceRef" check="false" registry="zk"></dubbo:reference>
    <dubbo:reference interface="com.lanxi.couponcode.spi.service.LoginService" id="loginControllerServiceRef" check="false" registry="zk"></dubbo:reference>
    <dubbo:reference interface="com.lanxi.couponcode.spi.service.OrderService" id="orderControllerServiceRef" check="false" registry="zk"></dubbo:reference>
    <dubbo:reference interface="com.lanxi.couponcode.spi.service.CommodityService" id="commodityControllerServiceRef" check="false" registry="zk"></dubbo:reference>
