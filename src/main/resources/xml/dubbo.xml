        http://www.springframework.org/schema/data/jpa
        http://www.springframework.org/schema/data/jpa/spring-jpa-1.8.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
     <!-- dubbo 服务配置 -->
    <dubbo:application name="coupon-code-all"></dubbo:application>
    <!-- 测试注册中心 -->
    <dubbo:registry address="zookeeper://192.168.17.154:2181" id="zk"></dubbo:registry>
    <!--<dubbo:registry address="redis://192.168.17.154:6379" id="rs"></dubbo:registry>-->
    <!-- 生产注册中心    
    <dubbo:registry address="zookeeper://10.23.73.28:2181"></dubbo:registry>
    -->
    <dubbo:protocol name="dubbo" port="20660" serialization="java"></dubbo:protocol>
    <!-- 蓝喜短信平台服务 -->
    <dubbo:reference interface="com.lanxi.common.interfaces.SmsSendServiceInterface" id="SmsService" check="false" registry="zk"></dubbo:reference>
    <dubbo:reference interface="com.lanxi.common.interfaces.RedisCacheServiceInterface" id="redisServiceOld" check="false" registry="zk"></dubbo:reference>

